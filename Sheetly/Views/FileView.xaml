<ItemsControl x:Class="Sheetly.Views.FileView"
              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" Name="fileDisplay"
              ItemsSource="{Binding}">
    <ItemsControl.ItemTemplate>
        <DataTemplate>
            <StackPanel Orientation="Horizontal">
                <StackPanel.Resources>
                    <Style TargetType="TextBlock">
                        <Setter Property="Foreground" Value="Gray" />
                        <Setter Property="FontSize" Value="16" />
                        <Setter Property="Margin" Value="25 5 25 5" />
                    </Style>
                </StackPanel.Resources>
                <TextBlock Text="HELLLOO"/>
                <TextBlock Text="{Binding Name}" />
                <TextBlock Text="{Binding FileName}" />
                <TextBlock Text="{Binding RowCount}" />
                <ComboBox Name="comboBox1"
                            ItemsSource="{Binding Headers}"
                            Text="Header Name"
                            IsEditable="true"
                            IsReadOnly="true"/>
            </StackPanel>
        </DataTemplate>
    </ItemsControl.ItemTemplate>
</ItemsControl>